{
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.fixAll.tslint": true,
      "source.fixAll.stylelint": true
  },
  "editor.tabSize": 2,
  "scss.lint.unknownAtRules": "ignore",
  "css.validate": true,
  "scss.validate": true,
  "files.associations": {
      "*.css": "scss"
  },
  "volar.tsPlugin": true,
  "volar.tsPluginStatus": false
}